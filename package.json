{
  "name": "json-view-editor",
  "version": "0.1.3",
  "author": "Raj Kumar Chauhan",
  "module": "dist/json-view-editor.esm.js",
  "size-limit": [
    {
      "path": "dist/json-view-editor.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/json-view-editor.esm.js",
      "limit": "10 KB"
    }
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postbuild": "purgecss --css dist/*.css --content dist/*.js --output dist"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.69.5",
    "size-limit": "^11.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.0",
    "@reduxjs/toolkit": "^1.9.7",
    "bootstrap": "^5.3.2",
    "clipboard": "^2.0.11",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "react-redux": "^8.1.3",
    "validator": "^13.11.0"
  },
  "overrides": {
    "rollup-plugin-import-css": {
      "rollup": "^1.32.1"
    }
  }
}
